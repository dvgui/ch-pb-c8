<%- include('./header')-%>

<body>
    <div class="container mt-3">
        <h1 class="header mt-3">Enter Data</h1>
        <form action="/products" method="post">
            <div class="form-group">
                <label for="title"><b>Name</b></label>
                <input class="form-control" type="text" id="title" name="title">
            </div>
            <div class="form-group">
                <label for="price"><b>Price</b></label>
                <input class="form-control" type="text" id="price" name="price">
            </div>
            <div class="form-group">
                <label for="image"><b>Image</b></label>
                <input class="form-control" type="text" id="image" name="thumbnail">
            </div>
            <button id="send" class="btn btn-primary mt-3 mb-5">Send</button>
        </form>
        <a href="/products" class="btn btn-primary mt-3"><p style="color: white;">View Products</p></a>
    </div>
</body>
</html>